<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ZBVCircleAvadarView">
        <attr name="resSrcId" format="reference" />
        <attr name="outBorderWidth" format="dimension" />
        <attr name="outBorderColor" format="color" />
        <attr name="needOutBorder" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="pwdMode" format="boolean" />
        <attr name="existShow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SummerLayout">
        <attr name="rowNum" format="integer" />
        <attr name="columnNum" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ToUsePullView">
        <attr name="pull_icon" format="reference" />
        <attr name="text_color" format="color" />
        <attr name="hint_color" format="color" />
        <attr name="hint_text" format="string" />
        <attr name="pop_menu_bg" format="color"></attr>
        <attr name="pop_text_color" format="color"></attr>
        <attr name="distance_txt_img" format="dimension"></attr>
        <attr name="pop_choosed_text_color" format="color"></attr>
        <attr name="is_pull_up" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="RoundCornerImageView">
        <attr name="borderRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="progress_value" format="integer" />
        <attr name="progress_color" format="color" />
        <attr name="background_color" format="color" />
        <attr name="progress_width" format="dimension" />
        <attr name="background_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BubbleView">
        <attr name="bubble_bg_color" format="color" />
        <attr name="bubble_text_size" format="dimension" />
        <attr name="bubble_text_color" format="color" />
        <attr name="bubble_min_value" format="integer" />
        <attr name="bubble_max_value" format="integer" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="lineSpacing" format="dimension" />
    </declare-styleable>


    <declare-styleable name="RippleBackground">
        <attr name="rb_color" format="color" />
        <attr name="rb_strokeWidth" format="dimension"/>
        <attr name="rb_radius" format="dimension"/>
        <attr name="rb_duration" format="integer"/>
        <attr name="rb_rippleAmount" format="integer"/>
        <attr name="rb_scale" format="float"/>
        <attr name="rb_type" format="enum">
            <enum name="fillRipple" value="0"/>
            <enum name="strokeRipple" value="1"/>
        </attr>
    </declare-styleable>
</resources>